<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="10" author="izekic">
        <createTable tableName="Breeds">
            <column name="id" type="int">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="breed_name" type="varchar(50)"/>
            <column name="height" type="varchar(20)"/>
            <column name="weight" type="varchar(20)"/>
            <column name="life_expectancy" type="varchar(20)"/>
            <column name="group" type="varchar(50)"/>
        </createTable>
    </changeSet>
</databaseChangeLog>